%../../../../../../../data/logics/hlm%

$Functions(
  X: %Set,
  Y: %Set
): %Construction {
  $function(_x: %Binder(
    #(x: %Element(X)),
    #(y: %Element(Y))
  )): %Constructor {
    equalityDefinition = {
      leftParameters = #(_x: %Binder(
        #(x: %Element(X)),
        #(y: %Element(Y))
      )),
      rightParameters = #(_x: %Binder(
        #(x: %Element(X)),
        #("y'": %Element(Y))
      )),
      definition = [%forall(
        #(x: %Element(X)),
        %equals(y[x = {x}], "y'"[x = {x}])
      )],
      isomorphic = %true
    }
  }
}
