%../../../../../../../data/logics/hlm%

$"less or equal"(
  m: %Element($"Natural numbers"),
  n: %Element(%previous)
): %Predicate {
  properties = {negation = $less(
    n = {m},
    m = {n}
  )},
  definition = [
    %exists(
      #(x: %Element($"Natural numbers")),
      formula = %equals(
        $sum(
          m = {m},
          n = {x}
        ),
        n
      )
    )
  ]
}
