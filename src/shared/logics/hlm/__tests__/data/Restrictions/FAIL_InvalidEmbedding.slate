%../../../../../../../data/logics/hlm%

/**
 * @expectedError Type mismatch: $../Essentials/"Pointed sets" and $FAIL_InvalidEmbedding are incompatible
 *
 * @expectedWarning Proof of %equals(s, s) required
 */
$FAIL_InvalidEmbedding: %Construction {
  $instance(
    S: %Set,
    s: %Element(S)
  ): %Constructor {
    equalityDefinition = {
      leftParameters = #(
        S: %Set,
        s: %Element(S)
      ),
      rightParameters = #(
        T: %Set,
        t: %Element(T)
      ),
      definition = [%exists(
        #(f: %Element($../Essentials/Bijections(
          X = {S},
          Y = {T}
        ))),
        formula = %equals(
          $../Essentials/value(
            X = {S},
            Y = {T},
            f = {f},
            x = {s}
          ),
          t
        )
      )],
      isomorphic = %true
    }
  }

  embedding = {
    parameter = #(s: %Element($../Essentials/"Natural numbers")),
    target = $../Essentials/"Pointed sets"."pointed set"(
        S = {$../Essentials/"Natural numbers"},
        s = {s}
    )
  }
}
