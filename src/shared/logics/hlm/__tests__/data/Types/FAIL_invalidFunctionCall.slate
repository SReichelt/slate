%../../../../../../../data/logics/hlm%

/**
 * @expectedError Type mismatch: element of $../Essentials/"Natural numbers" expected
 */
$FAIL_invalidFunctionCall(f: %Element($../Essentials/Functions(
  X = $../Essentials/"Natural numbers",
  Y = $../Essentials/"Pointed sets"
))): %Predicate {
  definition = [%equals(
    $../Essentials/value(
      X = $../Essentials/"Natural numbers",
      Y = $../Essentials/"Pointed sets",
      f = f,
      x = $../Essentials/"Pointed sets"."pointed set"(
        S = $../Essentials/"Natural numbers",
        s = $../Essentials/"Natural numbers".zero
      )
    ),
    $../Essentials/"Pointed sets"."pointed set"(
      S = $../Essentials/"Natural numbers",
      s = $../Essentials/"Natural numbers".zero
    )
  )]
}
