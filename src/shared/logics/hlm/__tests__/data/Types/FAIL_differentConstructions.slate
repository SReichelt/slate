/**
 * @expectedError Type mismatch: $../Essentials/"Pointed sets" and $../Essentials/"Natural numbers" are incompatible
 * @expectedError Type mismatch: $../Essentials/"Pointed sets" and $../Essentials/"Natural numbers" are incompatible
 * @expectedError Type mismatch: $../Essentials/"Pointed sets" and $../Essentials/"Natural numbers" are incompatible
 * @expectedError Type mismatch: $../Essentials/"Pointed sets" and $../Essentials/"Natural numbers" are incompatible
 */
$FAIL_differentConstructions(
  s: %Element($../Essentials/"Pointed sets"),
  t: %Element($../Essentials/"Natural numbers")
): %Predicate {
  definition = [
    %in(
      s,
      $../Essentials/"Natural numbers"
    ),
    %sub(
      $../Essentials/"Pointed sets",
      $../Essentials/"Natural numbers"
    ),
    %setEquals(
      $../Essentials/"Pointed sets",
      $../Essentials/"Natural numbers"
    ),
    %equals(s, t)
  ]
}
