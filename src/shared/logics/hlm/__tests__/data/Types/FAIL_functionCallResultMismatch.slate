%../../../../../../../data/logics/hlm%

/**
 * @expectedError Type mismatch
 */
$FAIL_functionCallResultMismatch(f: %Element($../Essentials/Functions(
  X = {$../Essentials/"Natural numbers"},
  Y = {$../Essentials/"Pointed sets"}
))): %Predicate {
  definition = [%equals(
    $../Essentials/value(
      X = {$../Essentials/"Natural numbers"},
      Y = {$../Essentials/"Pointed sets"},
      f = {f},
      x = {$../Essentials/"Natural numbers".zero}
    ),
    $../Essentials/"Natural numbers".zero
  )]
}
