$Feature(operand: %Expr, singular,plural: %Expr, article: %String = 'a'): %Template {
  notation = $Verb(
    verb = [%neg('has', 'does not have'), ' ', article, ' ', singular],
    operand = operand
  ),
  symbol = singular,
  elements = {
    operand = operand,
    singular = singular,
    plural = plural,
    article = article,
