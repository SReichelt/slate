%../../../../logics/hlm%

$commutative(𝔐: %Element(
  $Magmas,
  shortcut = {
    constructor = $Magmas.magma,
    parameters = #(
      M: %Set,
      ∗: %Element($../../Essentials/Functions/Operations/"Inner operations"(
        X = {M}
      ))
    )
  }
)): %Predicate {
  display = [$Property(
    operand = 𝔐,
    property = 'commutative'
  )],
  definition = [$../../Essentials/Functions/Operations/commutative(
    X = {M},
    Z = {M},
    ∗ = {∗}
  )]
}
