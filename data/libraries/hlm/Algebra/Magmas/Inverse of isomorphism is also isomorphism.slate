%../../../../logics/hlm%

$"Inverse of isomorphism is also isomorphism"(
  𝐌: %Element($Magmas),
  𝐍: %Element(%previous),
  φ: %Element($Isomorphisms(
    𝐌 = {𝐌},
    𝐍 = {𝐍}
  ))
): %StandardTheorem {
  claim = %in(
    $../../Essentials/Functions/inverse(
      X = {%setStructuralCases(
        𝐌,
        $Magmas,
        [{
          constructor = $Magmas.magma,
          parameters = #(
            M: %Set,
            ∗: %Element($../../Essentials/Functions/Operations/"Inner operations"(X = {M}))
          ),
          value = M
        }]
      )},
      Y = {%setStructuralCases(
        𝐍,
        $Magmas,
        [{
          constructor = $Magmas.magma,
          parameters = #(
            N: %Set,
            ⋆: %Element($../../Essentials/Functions/Operations/"Inner operations"(X = {N}))
          ),
          value = N
        }]
      )},
      f = {φ}
    ),
    $Isomorphisms(
      𝐌 = {𝐍},
      𝐍 = {𝐌}
    )
  )
}
