%../../../../../logics/hlm%

$"free magma"(
  S: %Set,
  T: %SetDef($"Binary trees"(S = {S}))
): %ExplicitOperator {
  display = [$Function(
    function = $SubSup(
      body = 'free',
      sub = '𝓜𝓰𝓶'
    ),
    arguments = [S]
  )],
  definition = [$../Magmas.magma(
      S = {T},
      ∗ = {$../../../Essentials/Functions/Functions(
          X = {$../../../Essentials/Sets/"Cartesian product"(
            S = {T},
            T = {T}
          )},
          Y = {T}
        ).function(x = {
          #(_x: %Element($../../../Essentials/Sets/"Cartesian product"(
            S = {T},
            T = {T}
          ))),
          {y = {%structuralCases(
            _x,
            $../../../Essentials/Sets/"Cartesian product"(
              S = {T},
              T = {T}
            ),
            [{
              constructor = $../../../Essentials/Sets/"Cartesian product"(
                  S = {T},
                  T = {T}
                ).pair,
              parameters = #(
                a: %Element(T),
                b: %Element(T)
              ),
              value = $"Binary trees"(S = {S}).join(
                  a = {a},
                  b = {b}
              )
            }]
          )}}
      })}
  )]
}
