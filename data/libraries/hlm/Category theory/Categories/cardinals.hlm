%../../../../logics/hlm%

/**
 * @remarks The objects of this category are actually isomorphism classes of sets, i.e. cardinal numbers.
 */
$cardinals: %ExplicitOperator {
  display = ['𝓢𝓮𝓽'],
  definition = [$Categories.category(
      O = {$../../Essentials/Numbers/Cardinal/"Cardinal numbers"},
      a = {
        #(𝔞: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
        {b = {
          #(𝔟: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
          {M = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
            𝔵 = {𝔞},
            𝔶 = {𝔟}
          )}}
        }}
      },
      c = {
        #(𝔠: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
        {i = {$../../Essentials/Numbers/Cardinal/identity(𝔵 = {𝔠})}}
      },
      d = {
        #(𝔡: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
        {e = {
          #(𝔢: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
          {f = {
            #(𝔣: %Element($../../Essentials/Numbers/Cardinal/"Cardinal numbers")),
            {∘ = {$../../Essentials/Functions/Operations/operator(
              X = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                𝔵 = {𝔢},
                𝔶 = {𝔣}
              )},
              Y = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                𝔵 = {𝔡},
                𝔶 = {𝔢}
              )},
              Z = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                𝔵 = {𝔡},
                𝔶 = {𝔣}
              )},
              parameters = #(
                φ: %Element($../../Essentials/Numbers/Cardinal/Homomorphisms(
                  𝔵 = {𝔢},
                  𝔶 = {𝔣}
                )),
                ψ: %Element($../../Essentials/Numbers/Cardinal/Homomorphisms(
                  𝔵 = {𝔡},
                  𝔶 = {𝔢}
                ))
              ),
              operator = $../../Essentials/Functions/composition(
                X = {%setStructuralCases(
                  𝔡,
                  $../../Essentials/Numbers/Cardinal/"Cardinal numbers",
                  [{
                    constructor = $../../Essentials/Numbers/Cardinal/"Cardinal numbers".cardinality,
                    parameters = #(X: %Set),
                    value = X
                  }]
                )},
                Y = {%setStructuralCases(
                  𝔢,
                  $../../Essentials/Numbers/Cardinal/"Cardinal numbers",
                  [{
                    constructor = $../../Essentials/Numbers/Cardinal/"Cardinal numbers".cardinality,
                    parameters = #(Y: %Set),
                    value = Y
                  }]
                )},
                Z = {%setStructuralCases(
                  𝔣,
                  $../../Essentials/Numbers/Cardinal/"Cardinal numbers",
                  [{
                    constructor = $../../Essentials/Numbers/Cardinal/"Cardinal numbers".cardinality,
                    parameters = #(Z: %Set),
                    value = Z
                  }]
                )},
                g = {φ},
                f = {ψ}
              )
            )}}
          }}
        }}
      }
  )]
}
