                    𝔵 = {𝔡},
                    𝔶 = {𝔢}
                  )}
                )},
                Y = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                  𝔵 = {𝔡},
                  𝔶 = {𝔣}
                )}
              ).function(x = {
                #(h: %Element(
                  $../../Essentials/Sets/"Cartesian product"(
                    S = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                      𝔵 = {𝔢},
                      𝔶 = {𝔣}
                    )},
                    T = {$../../Essentials/Numbers/Cardinal/Homomorphisms(
                      𝔵 = {𝔡},
                      𝔶 = {𝔢}
                    )}
