%../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Natural_transformation
 *             * https://ncatlab.org/nlab/show/natural+transformation
 */
$"Natural transformations"(
  U: %Symbol(auto = %true),
  𝓒: %Element(
    $../Categories(U = {U}),
    auto = %true
  ),
  𝓓: %Element(
    %previous,
    auto = %true
  ),
  F: %Element($../Functors/Functors(
    U = {U},
    𝓒 = {𝓒},
    𝓓 = {𝓓}
  )),
  G: %Element(%previous)
): %SetOperator {
  display = [$FunctionOperator(
    symbol = '⇒',
    operands = [F, G],
    minParenLevel = -1
  )],
  definitionDisplay = {
    parameter = #(η: %Expr),
    display = [$FunctionDefinition(
      function = η,
      symbol = '⇒',
      operands = [F, G]
    )],
    singularName = 'natural transformation',
    pluralName = 'natural transformations',
    nameOptional = %true
  },
  definition = [%subset(
    #(η: %Element($"Infranatural transformations"(
      U = {U},
      𝓒 = {𝓒},
      𝓓 = {𝓓},
      F = {F},
      G = {G}
    ))),
    %forall(
      #(
        𝐗: %Element($../Objects(
          U = {U},
          𝓒 = {𝓒}
        )),
        𝐘: %Element(%previous),
        f: %Element($../Morphisms(
          U = {U},
          𝓒 = {𝓒},
          𝐗 = {𝐗},
          𝐘 = {𝐘}
        ))
      ),
      %equals(
        $../composition(
          U = {U},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐘 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐘}
          )},
          𝐙 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐘}
          )},
          f = {$../Functors/"morphism value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐘 = {𝐗},
            𝐙 = {𝐘},
            f = {f}
          )},
          g = {$component(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            G = {G},
            η = {η},
            𝐗 = {𝐘}
          )}
        ),
        $../composition(
          U = {U},
          𝓒 = {𝓓},
          𝐗 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐘 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐗}
          )},
          𝐙 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐗 = {𝐘}
          )},
          f = {$component(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            G = {G},
            η = {η},
            𝐗 = {𝐗}
          )},
          g = {$../Functors/"morphism value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {G},
            𝐘 = {𝐗},
            𝐙 = {𝐘},
            f = {f}
          )}
        )
      )
    )
  )]
}
