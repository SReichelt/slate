%../../../../logics/hlm%

$identity(
  U: %Symbol(auto = %true),
  𝓒: %Element(
    $../Categories(U = {U}),
    auto = %true
  ),
  𝓓: %Element(
    %previous,
    auto = %true
  ),
  F: %Element(
    $../Functors/Functors(
      U = {U},
      𝓒 = {𝓒},
      𝓓 = {𝓓}
    ),
    auto = %true
  )
): %ExplicitOperator {
  display = [$SubSup(
    body = 'id',
    sub = F
  )],
  definition = [
    $"Infranatural transformations"(
        U = {U},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(𝐗 = {
        parameter = #(𝐗: %Element($../Objects(
          U = {U},
          𝓒 = {𝓒}
        ))),
        arguments = {n = {$../identity(
          U = {U},
          𝓒 = {𝓒},
          𝐗 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )}
        )}}
    }),
    $"Infranatural transformations"(
        U = {U},
        𝓒 = {𝓒},
        𝓓 = {𝓓},
        F = {F},
        G = {F}
      ).transformation(𝐗 = {
        parameter = #(𝐗: %Element($../Objects(
          U = {U},
          𝓒 = {𝓒}
        ))),
        arguments = {n = {$../Functors/"morphism value"(
          U = {U},
          𝓒 = {𝓒},
          𝓓 = {𝓓},
          F = {F},
          𝐘 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          𝐙 = {$../Functors/"object value"(
            U = {U},
            𝓒 = {𝓒},
            𝓓 = {𝓓},
            F = {F},
            𝐗 = {𝐗}
          )},
          f = {$../identity(
            U = {U},
            𝓒 = {𝓒},
            𝐗 = {𝐗}
          )}
        )}}
    })
  ]
}
