%../../../../logics/hlm%

/**
 * @references * https://proofwiki.org/wiki/Union_is_Commutative
 */
$"Union is commutative"(
  U: %Set,
  S: %Subset(U),
  T: %Subset(%previous)
): %StandardTheorem {
  claim = %setEquals(
    $Union(
      U = {U},
      S = {S},
      T = {T}
    ),
    $Union(
      U = {U},
      S = {T},
      T = {S}
    )
  )
}
