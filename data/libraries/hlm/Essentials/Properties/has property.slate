%../../../../logics/hlm%

$"has property"(
  S: %Set(auto = %true),
  p: %Element(
    $Properties(S = {S}),
    shortcut = {
      constructor = $Properties(S = {S}).property,
      parameters = #(P: %Subset(S))
    }
  ),
  s: %Element(S)
): %Predicate {
  display = [$TextualRelation(
    symbol = %neg('is', 'is not'),
    operands = [s, p]
  )],
  definition = [%in(s, P)]
}
