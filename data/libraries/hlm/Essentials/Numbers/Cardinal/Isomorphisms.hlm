%../../../../../logics/hlm%

$Isomorphisms(
  𝔵: %Element(
    $"Cardinal numbers",
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(X: %Set)
    }
  ),
  𝔶: %Element(
    %previous,
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(Y: %Set)
    }
  )
): %SetOperator {
  display = [$Function(
    function = 'Iso',
    arguments = [𝔵, 𝔶]
  )],
  definition = [
    %subset(
      #(φ: %Element($Homomorphisms(
        𝔵 = {𝔵},
        𝔶 = {𝔶}
      ))),
      $../../Functions/bijective(
        X = {X},
        Y = {Y},
        f = {φ}
      )
    ),
    $../../Functions/Bijections(
      X = {X},
      Y = {Y}
    )
  ]
}
