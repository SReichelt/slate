%../../../../../logics/hlm%

$power(
  𝔵: %Element(
    $"Cardinal numbers",
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(X: %Set)
    }
  ),
  𝔶: %Element(
    %previous,
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(Y: %Set)
    }
  )
): %ExplicitOperator {
  display = [$Power(
    base = 𝔵,
    exponent = 𝔶
  )],
  definition = [$"Cardinal numbers".cardinality(S = {$../../Functions/Functions(
      X = {Y},
      Y = {X}
  )})]
}
