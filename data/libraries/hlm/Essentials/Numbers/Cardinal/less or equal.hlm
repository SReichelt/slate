%../../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Cardinal_number
 */
$"less or equal"(
  𝔵: %Element($"Cardinal numbers"),
  𝔶: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('≤', '≰'),
      operands = [𝔵, 𝔶]
    ),
    $Relation(
      symbol = %neg('≥', '≱'),
      operands = [𝔶, 𝔵]
    )
  ],
  definition = [%exists(
    #(φ: %Element($Homomorphisms(
      𝔵 = {𝔵},
      𝔶 = {𝔶}
    ))),
    formula = $../../Functions/injective(
      X = {%setStructuralCases(
        𝔵,
        $"Cardinal numbers",
        [{
          constructor = $"Cardinal numbers".cardinality,
          parameters = #(X: %Set),
          value = X
        }]
      )},
      Y = {%setStructuralCases(
        𝔶,
        $"Cardinal numbers",
        [{
          constructor = $"Cardinal numbers".cardinality,
          parameters = #(Y: %Set),
          value = Y
        }]
      )},
      f = {φ}
    )
  )]
}
