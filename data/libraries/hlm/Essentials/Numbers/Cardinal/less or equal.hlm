%../../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Cardinal_number
 */
$"less or equal"(
  𝐱: %Element($"Cardinal numbers"),
  𝐲: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('≤', '≰'),
      operands = [𝐱, 𝐲]
    ),
    $Relation(
      symbol = %neg('≥', '≱'),
      operands = [𝐲, 𝐱]
    )
  ],
  definition = [%exists(
    #(φ: %Element($Homomorphisms(
      𝐱 = {𝐱},
      𝐲 = {𝐲}
    ))),
    formula = $../../Functions/injective(
      X = {%setStructuralCases(
        𝐱,
        $"Cardinal numbers",
        [{
          constructor = $"Cardinal numbers".cardinality,
          parameters = #(X: %Set),
          value = X
        }]
      )},
      Y = {%setStructuralCases(
        𝐲,
        $"Cardinal numbers",
        [{
          constructor = $"Cardinal numbers".cardinality,
          parameters = #(Y: %Set),
          value = Y
        }]
      )},
      f = {φ}
    )
  )]
}
