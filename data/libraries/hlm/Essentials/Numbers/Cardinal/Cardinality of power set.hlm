%../../../../../logics/hlm%

$"Cardinality of power set"(S: %Set): %StandardTheorem {
  claim = %equals(
    $"Cardinal numbers".cardinality(S = {$../../Sets/"Power set"(S = {S})}),
    $power(
      𝔵 = {$../Natural/number(value = 2)},
      𝔶 = {$"Cardinal numbers".cardinality(S = {S})}
    )
  )
}
