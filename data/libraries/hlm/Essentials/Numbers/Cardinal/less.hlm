%../../../../../logics/hlm%

/**
 * @remarks Note that without the axiom of choice, this is not equivalent to the negation of “greater or equal”.
 */
$less(
  𝔵: %Element($"Cardinal numbers"),
  𝔶: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('<', '≮'),
      operands = [𝔵, 𝔶]
    ),
    $Relation(
      symbol = %neg('>', '≯'),
      operands = [𝔶, 𝔵]
    )
  ],
  definition = [%and(
    $"less or equal"(
      𝔵 = {𝔵},
      𝔶 = {𝔶}
    ),
    %not(%equals(𝔵, 𝔶))
  )]
}
