%../../../../../logics/hlm%

/**
 * @remarks Note that without the axiom of choice, this is not equivalent to the negation of “greater or equal”.
 */
$less(
  𝐱: %Element($"Cardinal numbers"),
  𝐲: %Element(%previous)
): %Predicate {
  display = [
    $Relation(
      symbol = %neg('<', '≮'),
      operands = [𝐱, 𝐲]
    ),
    $Relation(
      symbol = %neg('>', '≯'),
      operands = [𝐲, 𝐱]
    )
  ],
  definition = [%and(
    $"less or equal"(
      𝐱 = {𝐱},
      𝐲 = {𝐲}
    ),
    %not(%equals(𝐱, 𝐲))
  )]
}
