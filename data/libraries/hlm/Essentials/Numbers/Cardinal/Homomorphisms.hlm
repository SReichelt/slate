%../../../../../logics/hlm%

$Homomorphisms(
  𝔵: %Element(
    $"Cardinal numbers",
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(X: %Set)
    }
  ),
  𝔶: %Element(
    %previous,
    shortcut = {
      constructor = $"Cardinal numbers".cardinality,
      parameters = #(Y: %Set)
    }
  )
): %SetOperator {
  display = [$Function(
    function = 'Hom',
    arguments = [𝔵, 𝔶]
  )],
  definitionDisplay = {
    parameter = #(φ: %Expr),
    display = [$FunctionDefinition(
      function = φ,
      symbol = '→',
      operands = [𝔵, 𝔶]
    )],
    singularName = 'homomorphism',
    pluralName = 'homomorphisms',
    nameOptional = %true
  },
  definition = [$../../Functions/Functions(
    X = {X},
    Y = {Y}
  )]
}
