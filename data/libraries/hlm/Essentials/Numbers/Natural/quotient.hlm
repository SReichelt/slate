%../../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Quotient
 */
$quotient(
  n: %Element($"Natural numbers"),
  m: %Element($Divisors(n = {n}))
): %ImplicitOperator {
  display = [$Fraction(
    numerator = n,
    denominator = m
  )],
  parameter = #(x: %Element($"Natural numbers")),
  definition = [%equals(
    n,
    $product(
      m = {x},
      n = {m}
    )
  )]
}
