%../../../../../../logics/hlm%

$"Every number greater than one has a prime factor"(n: %Element($../Subsets/"Segment (greater)"(n = {$../number(value = 1)}))): %StandardTheorem {
  claim = %exists(
    #(p: %Element($"Prime numbers")),
    formula = $../divisible(
      m = {p},
      n = {n}
    )
  )
}
