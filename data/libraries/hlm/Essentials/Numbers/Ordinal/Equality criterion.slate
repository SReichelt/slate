%../../../../../logics/hlm%

$"Equality criterion"(
  ğ±: %Element($"Ordinal numbers"),
  ğ²: %Element(%previous)
): %EquivalenceTheorem {
  conditions = [
    %and(
      $"less or equal"(
        ğ± = {ğ±},
        ğ² = {ğ²}
      ),
      $"less or equal"(
        ğ± = {ğ²},
        ğ² = {ğ±}
      )
    ),
    %not($../../../Essentials/Sets/empty(S = {$Isomorphisms(
      ğ± = {ğ±},
      ğ² = {ğ²}
    )})),
    %equals(ğ±, ğ²)
  ]
}
