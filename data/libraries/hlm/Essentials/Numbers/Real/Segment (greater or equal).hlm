%../../../../../logics/hlm%

$"Segment (greater or equal)"(a: %Element($"Real numbers")): %SetOperator {
  definition = [
    %subset(
      #(b: %Element($"Real numbers")),
      $"less or equal"(
        b = {b},
        a = {a}
      )
    ),
    $../../Sets/"Relative complement"(
      S = {$"Real numbers"},
      T = {$"Segment (less)"(a = {a})}
    )
  ]
}
