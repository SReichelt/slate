%../../../../../../logics/hlm%

$"Segment (greater or equal)"(a: %Element($../"Real numbers")): %SetOperator {
  display = [$Segment(
    set = 'ℝ',
    symbol = '≥',
    limit = a
  )],
  definition = [
    %subset(
      #(b: %Element($../"Real numbers")),
      $../"less or equal"(
        b = {b},
        a = {a}
      )
    ),
    $../../../Sets/"Relative complement"(
      U = {$../"Real numbers"},
      S = {$../"Real numbers"},
      T = {$"Segment (less)"(a = {a})}
    )
  ]
}
