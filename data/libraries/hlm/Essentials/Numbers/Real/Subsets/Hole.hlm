%../../../../../../logics/hlm%

$Hole(a: %Element($../"Real numbers")): %SetOperator {
  display = [$Segment(
    set = 'ℝ',
    symbol = '≠',
    limit = a
  )],
  definition = [
    %subset(
      #(b: %Element($../"Real numbers")),
      %not(%equals(b, a))
    ),
    $../../../Sets/Union(
      U = {$../"Real numbers"},
      S = {$"Segment (less)"(a = {a})},
      T = {$"Segment (greater)"(a = {a})}
    )
  ]
}
