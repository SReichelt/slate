%../../../../../logics/hlm%

$quotient(
  b: %Element($"Real numbers"),
  a: %Element($Subsets/"Nonzero numbers")
): %ImplicitOperator {
  display = [$Fraction(
    numerator = b,
    denominator = a
  )],
  parameter = #(x: %Element($"Real numbers")),
  definition = [%equals(
    b,
    $product(
      a = {x},
      b = {a}
    )
  )]
}
