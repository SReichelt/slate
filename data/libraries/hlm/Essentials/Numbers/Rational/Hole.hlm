%../../../../../logics/hlm%

$Hole(a: %Element($"Rational numbers")): %SetOperator {
  display = [$Segment(
    set = 'ℚ',
    symbol = '≠',
    limit = a
  )],
  definition = [
    %subset(
      #(b: %Element($"Rational numbers")),
      %not(%equals(b, a))
    ),
    $../../Sets/Union(
      U = {$"Rational numbers"},
      S = {$"Segment (less)"(a = {a})},
      T = {$"Segment (greater)"(a = {a})}
    )
  ]
}
