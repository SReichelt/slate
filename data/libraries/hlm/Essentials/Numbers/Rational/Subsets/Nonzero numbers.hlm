%../../../../../../logics/hlm%

$"Nonzero numbers": %SetOperator {
  display = [$SubSup(
    body = 'ℚ',
    sub = '±'
  )],
  definition = [
    $Hole(a = {$../../Natural/number(value = 0)}),
    %subset(
      #(a: %Element($../"Rational numbers")),
      %not(%equals(
        a,
        $../../Natural/number(value = 0)
      ))
    ),
    $../../../Sets/Union(
      U = {$../"Rational numbers"},
      S = {$"Negative numbers"},
      T = {$"Positive numbers"}
    )
  ]
}
