%../../../../../logics/hlm%

$"Segment (greater or equal)"(a: %Element($"Rational numbers")): %SetOperator {
  display = [$Segment(
    set = 'ℚ',
    symbol = '≥',
    limit = a
  )],
  definition = [
    %subset(
      #(b: %Element($"Rational numbers")),
      $"less or equal"(
        b = {b},
        a = {a}
      )
    ),
    $../../Sets/"Relative complement"(
      U = {$"Rational numbers"},
      S = {$"Rational numbers"},
      T = {$"Segment (less)"(a = {a})}
    )
  ]
}
