%../../../../../logics/hlm%

$less(
  a: %Element($"Rational numbers"),
  b: %Element(%previous)
): %Predicate {
  definition = [
    %not($"less or equal"(
      b = {a},
      a = {b}
    )),
    %and(
      $"less or equal"(
        a = {a},
        b = {b}
      ),
      %not(%equals(a, b))
    )
  ]
}
