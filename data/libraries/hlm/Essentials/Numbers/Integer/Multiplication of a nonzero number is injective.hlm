%../../../../../logics/hlm%

$"Multiplication of a nonzero number is injective"(
  a: %Element($Integers),
  b: %Element(%previous),
  c: %Element($"Nonzero numbers"),
  _1: %Constraint(%equals(
    $product(
      a = {a},
      b = {c}
    ),
    $product(
      a = {b},
      b = {c}
    )
  ))
): %StandardTheorem {
  claim = %equals(a, b)
}
