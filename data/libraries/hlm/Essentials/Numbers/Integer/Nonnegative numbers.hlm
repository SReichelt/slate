%../../../../../logics/hlm%

$"Nonnegative numbers": %SetOperator {
  definition = [
    $"Segment (greater or equal)"(a = {$../Natural/number(value = 0)}),
    %subset(
      #(a: %Element($Integers)),
      $"less or equal"(
        b = {a},
        a = {$../Natural/number(value = 0)}
      )
    ),
    $../Natural/"Natural numbers"
  ]
}
