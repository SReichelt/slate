%../../../../../logics/hlm%

$quotient(
  b: %Element($Integers),
  a: %Element($Divisors(b = {b}))
): %ImplicitOperator {
  display = [$Quotient(
    numerator = b,
    denominator = a
  )],
  parameter = #(x: %Element($Integers)),
  definition = [%equals(
    b,
    $product(
      a = {x},
      b = {a}
    )
  )]
}
