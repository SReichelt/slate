%../../../../../logics/hlm%

$"Sum is associative"(
  a: %Element($Integers),
  b: %Element(%previous),
  c: %Element(%previous)
): %StandardTheorem {
  claim = %equals(
    $sum(
      a = {$sum(
        a = {a},
        b = {b}
      )},
      b = {c}
    ),
    $sum(
      a = {a},
      b = {$sum(
        a = {b},
        b = {c}
      )}
    )
  )
}
