%../../../../../logics/hlm%

$"Positive numbers": %SetOperator {
  definition = [
    $"Segment (greater)"(a = {$../Natural/number(value = 0)}),
    %subset(
      #(a: %Element($Integers)),
      $less(
        b = {a},
        a = {$../Natural/number(value = 0)}
      )
    ),
    $../Natural/"Positive numbers"
  ]
}
