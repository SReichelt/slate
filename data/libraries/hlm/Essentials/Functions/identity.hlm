%../../../../logics/hlm%

/**
 * @references * https://en.wikipedia.org/wiki/Identity_function
 *             * https://proofwiki.org/wiki/Definition:Identity_Mapping
 */
$identity(X: %Set(auto = %true)): %ExplicitOperator {
  display = [$SubSup(
    body = 'id',
    sub = X
  )],
  definition = [$Functions(
      X = {X},
      Y = {X}
    ).function(x = {
      #(x: %Element(X)),
      {y = {x}}
  })]
}
