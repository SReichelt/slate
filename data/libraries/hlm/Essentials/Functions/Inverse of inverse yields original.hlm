%../../../../logics/hlm%

$"Inverse of inverse yields original"(
  X: %Set(auto = %true),
  Y: %Set(auto = %true),
  f: %Element($Bijections(
    X = {X},
    Y = {Y}
  ))
): %StandardTheorem {
  claim = %equals(
    $inverse(
      X = {Y},
      Y = {X},
      f = {$inverse(
        X = {X},
        Y = {Y},
        f = {f}
      )}
    ),
    f
  )
}
