%../../../../logics/hlm%

$"Application of function after inverse is neutral"(
  X: %Set(auto = %true),
  Y: %Set(auto = %true),
  f: %Element($Bijections(
    X = {X},
    Y = {Y}
  )),
  y: %Element(Y)
): %StandardTheorem {
  claim = %equals(
    $value(
      X = {X},
      Y = {Y},
      f = {f},
      x = {$value(
        X = {Y},
        Y = {X},
        f = {$inverse(
          X = {X},
          Y = {Y},
          f = {f}
        )},
        x = {y}
      )}
    ),
    y
  )
}
