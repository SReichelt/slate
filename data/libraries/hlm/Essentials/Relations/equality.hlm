%../../../../logics/hlm%

$equality(S: %Set): %ExplicitOperator {
  display = [$SubSup(
    body = '=',
    sub = S
  )],
  definition = [$Relations(S = {S}).relation(R = {%extendedSubset(
      #(s: %Element(S)),
      $../Sets/"Cartesian product"(
          S = {S},
          T = {S}
        ).pair(
          s = {s},
          t = {s}
      )
  )})]
}
