%../../../../logics/hlm%

$preorder(
  S: %Set(auto = %true),
  ≺: %Element($Relations(S = {S}))
): %Predicate {
  display = [$Property(
    property = 'preorder',
    operand = ≺,
    article = 'a'
  )],
  definition = [%and(
    $reflexive(
      S = {S},
      ≺ = {≺}
    ),
    $transitive(
      S = {S},
      ≺ = {≺}
    )
  )]
}
