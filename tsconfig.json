{
  "compilerOptions": {
    "outDir": "build",
    "target": "es6",
    "sourceMap": true,
    "module": "commonjs",
    "importHelpers": true,
    "allowJs": true,
    "lib": ["es6"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true
  },
  "files": [
    "src/shared/logics/logics.ts"
  ]
}
